<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconView">
        <attr name="iconString" format="string" />
        <attr name="color" format="color" />
        <attr name="iconName" format="string" />
    </declare-styleable>

    <declare-styleable name="IconTextView">
        <attr name="iconType" format="enum">
            <enum name="regular" value="0" />
            <enum name="solid" value="1" />
            <enum name="brands" value="2" />
        </attr>

    </declare-styleable>
</resources>